/* Load dataset 1 from csv */
d1 := 'csv:ecbexrusd_vtl.csv';
result:=  d1[rename CURRENCY to Me_3, EXR_SUFFIX to Me_4, CURRENCY_DENOM to Me_5, FREQ to Me_6, EXR_TYPE to Me_7];
ref_output := 'csv:rename.csv';
tmp_result1 := (result#Me_3 = ref_output#Me_3)[rename bool_var to res1];
tmp_result2 := (result#Me_4 = ref_output#Me_4)[rename bool_var to res2];
tmp_result3 := (result#Me_5 = ref_output#Me_5)[rename bool_var to res3];
tmp_result4 := (result#Me_6 = ref_output#Me_6)[rename bool_var to res4];
tmp_result5 := (result#Me_7 = ref_output#Me_7)[rename bool_var to res5];
test_result := inner_join(tmp_result1, tmp_result2, tmp_result3, tmp_result4, tmp_result5);