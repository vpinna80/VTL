/* Load dataset 1 from csv */
ds1:= 'csv:lag.csv';
result:= fill_time_series(ds1 , single)[calc previous := lag(edf1, 1 over (partition by mkmvid order by edfdate))];
ref_output := 'csv:lag_2.csv';
tmp_result1 := (result#edf1 = ref_output#edf1)[rename bool_var to res1];
tmp_result2 := (result#previous = ref_output#previous)[rename bool_var to res2];
test_result := inner_join(tmp_result1, tmp_result2);