/* Load dataset 1 from csv */
ds1:= 'csv:number_test7.csv';
result:= ds1[calc Num_ceil:= ceil (Num_2), Num_floor:= floor (Num_2)];
ref_output := 'csv:ceil_floor_1.csv';
tmp_result1 := (result#Num_ceil = ref_output#Num_ceil)[rename bool_var to res1];
tmp_result2 := (result#Num_floor = ref_output#Num_floor)[rename bool_var to res2];
test_result := inner_join(tmp_result1, tmp_result2);

