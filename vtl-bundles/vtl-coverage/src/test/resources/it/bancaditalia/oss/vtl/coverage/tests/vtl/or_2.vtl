/* Load dataset 1 from csv */
ds1:= 'csv:boolean_test4.csv';
result:= ds1[calc orT:= Me_1 or true, orF:= Me_1 or false];
ref_output := 'csv:or_2.csv';
tmp_result1 := (result#orT = ref_output#orT)[rename bool_var to res1];
tmp_result2 := (result#orF = ref_output#orF)[rename bool_var to res2];
test_result := inner_join(tmp_result1, tmp_result2);