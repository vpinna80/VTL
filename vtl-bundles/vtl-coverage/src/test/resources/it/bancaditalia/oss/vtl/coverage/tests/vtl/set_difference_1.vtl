/* Load dataset 1 from csv */
ds1:= 'csv:intersection_test1.csv';
/* Load dataset 2 from csv */
ds2:= 'csv:intersection_test2.csv';
result:= setdiff(ds1,ds2);
ref_output := 'csv:set_diff_1.csv';
tmp_result1 := (result#edf1 = ref_output#edf1)[rename bool_var to res1];
tmp_result2 := (result#edf2 = ref_output#edf2)[rename bool_var to res2];
tmp_result3 := (result#edf3 = ref_output#edf3)[rename bool_var to res3];
test_result := inner_join(tmp_result1, tmp_result2, tmp_result3);