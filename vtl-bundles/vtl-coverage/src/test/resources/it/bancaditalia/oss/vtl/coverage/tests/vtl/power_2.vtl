/* Load dataset 1 from csv */
ds1:= 'csv:number_test7.csv';
result:= ds1[calc Pow_1:= power (Num_1,2), Pow_2:= power (Num_2,Num_1)];
ref_output := 'csv:power_2.csv';
tmp_result1 := (result#Pow_1 = ref_output#Pow_1)[rename bool_var to res1];
tmp_result2 := (round(result#Pow_2, 5) = round(ref_output#Pow_2, 5))[rename bool_var to res2];
test_result := inner_join(tmp_result1, tmp_result2);
