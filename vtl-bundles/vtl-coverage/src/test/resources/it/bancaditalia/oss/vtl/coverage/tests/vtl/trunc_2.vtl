/* Load dataset 1 from csv */
ds1:= 'csv:number_test7.csv';
result:= ds1[calc roun:= round(Num_2, 1), trun:= trunc(Num_2, 1)];
ref_output := 'csv:trunc_2.csv';
tmp_result1 := (result#roun = ref_output#roun)[rename bool_var to res1];
tmp_result2 := (result#trun = ref_output#trun)[rename bool_var to res2];
test_result := inner_join(tmp_result1, tmp_result2);

