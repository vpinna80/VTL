/* Load dataset 1 from csv */
ds1:= 'csv:boolean_test6.csv';
result:= ds1[calc Me_and:= Me_1 and Me_2, Me_or:= Me_1 or Me_2];
ref_output := 'csv:and_or.csv';
tmp_result1 := (result#Me_and = ref_output#Me_and)[rename bool_var to res1];
tmp_result2 := (result#Me_or = ref_output#Me_or)[rename bool_var to res2];
test_result := inner_join(tmp_result1, tmp_result2);