	<profiles>
		<profile>
			<id>default</id>
			<activation>
				<property>
					<name>!vtl.skip.default</name>
				</property>
			</activation>
			<modules>
				<module>vtl-api</module>
				<module>vtl-bundles</module>
				<module>vtl-conf</module>
				<module>vtl-engine</module>
				<module>vtl-envs</module>
				<module>vtl-meta</module>
				<module>vtl-parsers</module>
				<module>vtl-samples</module>
				<module>vtl-session</module>
				<module>vtl-transform</module>
				<module>vtl-types</module>
			</modules>
		</profile>
		<profile>
			<id>full-reporting</id>
			<activation>
				<property>
					<name>parent.relativePath</name>
				</property>
			</activation>
			<reporting>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-project-info-reports-plugin</artifactId>
						<reportSets>
							<reportSet>
								<reports>
									<report>summary</report>
									<report>dependency-info</report>
									<report>dependencies</report>
									<report>modules</report>
									<report>plugins</report>

									<!-- parent specific reports -->
									<report>dependency-management</report>
									<report>plugin-management</report>
									<report>ci-management</report>
									<report>dependency-convergence</report>
									<report>issue-management</report>
									<report>licenses</report>
									<report>scm</report>
									<report>team</report>
								</reports>
							</reportSet>
						</reportSets>
					</plugin>
				</plugins>
			</reporting>
		</profile>
		<profile>
			<id>with-r</id>
		</profile>
		<profile>
			<id>with-sdmx</id>
		</profile>
		<profile>
			<id>with-spark</id>
		</profile>
		<profile>
			<id>with-cli</id>
		</profile>
		<profile>
